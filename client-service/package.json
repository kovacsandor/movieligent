{
  "name": "client-service",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "1.5.1",
    "openapi-fetch": "0.7.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.3",
    "react-scripts": "5.0.1",
    "shared": "../shared",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.5.2",
    "@types/node": "16.18.54",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "concurrently": "8.2.1",
    "openapi-typescript": "6.7.0",
    "tailwindcss": "3.3.3",
    "typescript": "4.9.5"
  },
  "scripts": {
    "openapi-generate": "openapi-typescript \"node_modules/shared/src/openapi/**/*.yaml\" --output ./ --immutable-types --export-type",
    "prestart": "npm run openapi-generate",
    "start": "concurrently \"npm run tailwindcss-dev\" \"react-scripts start\"",
    "build": "tailwindcss-build && react-scripts build",
    "tailwindcss-build": "tailwindcss -i ./src/index.css -o ./dist/index.css",
    "tailwindcss-dev": "tailwindcss -i ./src/index.css -o ./dist/index.css --watch",
    "test": "CI=true && react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
